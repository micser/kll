Name = minitel2_mouse;
Version = 0.1;
Author = "Michael Serisier 2016";
KLL = 0.3d;

# Modified Date
Date = 2016-11-04;


# Keypad
S0x2 : mouseOut( 1, 0, 0 ); # 1: left click
S0x7 : mouseOut( 2, 0, 0 ); # 3: right click
S0xC : mouseOut( 3, 0, 0 ); # 5: middle click

S0x4 : mouseOut( 0, 0, -3 ); # 2: Up
S0x14 : mouseOut( 0, 0, 3 ); # 8: Down
S0xA : mouseOut( 0, -3, 0 ); # 4: Left
S0xF : mouseOut( 0, 3, 0 ); # 6: Right
